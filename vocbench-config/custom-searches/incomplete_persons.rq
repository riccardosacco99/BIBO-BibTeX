PREFIX foaf: <http://xmlns.com/foaf/0.1/>

# incomplete_persons.rq
# Persone senza nome completo (solo foaf:name, manca given/family)
SELECT ?person ?name WHERE {
  ?person a foaf:Person ;
          foaf:name ?name .
  FILTER NOT EXISTS { ?person foaf:givenName ?given }
  FILTER NOT EXISTS { ?person foaf:familyName ?family }
}
