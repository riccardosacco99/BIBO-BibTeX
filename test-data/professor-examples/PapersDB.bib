@InProceedings{2108_67583,
  author    = {Pazienza, Maria Teresa and Stellato, Armando and Tudorache, Alexandra Gabriela and Turbati, Andrea and Vagnoni, Flaminia},
  title     = {An Architecture for Data and Knowledge Acquisition for the Semantic Web: the AGROVOC Use Case},
  booktitle = {Proceedeings of the 8th International Workshop on Ontology Content (OnToContent 2012)},
  year      = {2012},
  editor    = {Springer Verlag},
  volume    = {7567},
  series    = {Lecture Notes in Computer Science},
  pages     = {426-433},
  address   = {Rome, Italy},
  month     = {September},
  abstract  = {We are surrounded by ever growing volumes of unstructured and weakly-structured information, and for a human being, either domain expert or not, it is nearly impossible to read, understand and categorize it in a fair amount of time. Moreover, different user categories have different expectations: final users need easy to use tools and services for specific tasks, knowledge engineers require robust tools for knowledge acquisition, knowledge categorization and semantic resources development, while semantic applications developers demand for flexible frameworks for fast and easy, standardized development of complex applications. The proposed system is based on CODA open architecture and demonstrates the hands-on experience of building a complete data acquisition workflow, including data extraction and transformation (triplification) for AGROVOC thesaurus enrichment. The system integrates, with an high level of autonomy, different NLP tools and custom libraries, following UIMA standards. In this perspective, the architecture, the implementation and experimental results will be discussed.},
  doi       = {10.1007/978-3-642-33618-8_58},
  file      = {:C\:\\DEVELOPMENT\\WEBROOT\\art\\publications\\docs\\2012_OnToContent_An Architecture for Data and Knowledge Acquisition for the Semantic Web - the AGROVOC Use Case.pdf:PDF},
  groups    = {StellatoExport},
  isbn      = {978-3-642-33617-1},
  keywords  = {ontology population, relation extraction, unstructured data acquisition, CODA, UIMA annotators},
  url       = {http://link.springer.com/chapter/10.1007%2F978-3-642-33618-8_58},
}

