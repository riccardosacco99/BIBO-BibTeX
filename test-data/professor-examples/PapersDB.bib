@InProceedings{CueHorrKaz+07,
  author    = {Bernardo Cuenca Grau and Ian Horrocks and Yevgeny Kazakov and Ulrike Sattler},
  title     = {{Just the Right Amount: Extracting Modules from Ontologies}},
  booktitle = {{WWW '07: Proceedings of the 16th international conference on World Wide Web}},
  year      = {2007},
  pages     = {717--726},
  address   = {New York, NY, USA},
  publisher = {ACM Press},
  comment   = {The authors present an approach to extract modules from an ontology based on a definition of module that guarantees to completely capture the meaning of a given set of terms and study the problem of extracting minimal modules covering the meaning of the given set of terms. The focus of this paper is in the use of modularity for the partial reuse of ontologies, or more precisely, of modules extracted from the foreing ontology.
Due to undecidability problems in determining if a subset of an ontology is a module for a given vocabulary two approximations are proposed, one is of semantic nautre and can be computed with existing DL reasoners, the other is syntactic and can be computed in polynomial time.
In a previous work the authors defined what it means for an ontology to be safely integrated with a "foreign" ontology: roughly speaking such an integration is safe if it does not change the meaning of the terms in the foreign ontology.
The authors give main contributions in four points as they state:
1) propose a definition of a module Q1 within a given ontology Q for a given vocabulary S
2) investigate the problem of computing minimal modules, showing that none of the reasonable variants of this problem is solvable in general already for rather restricted sub-languages of OWL DL.
3) propose two aproximations that, are perhaps not producing minimal modules, but are computable
4) Describe their implementation and experimental results on a real world set of ontologies and compare their results with other module-extraction approaches.},
  doi       = {http://doi.acm.org/10.1145/1242572.1242669},
  isbn      = {978-1-59593-654-7},
  location  = {Banff, Alberta, Canada},
  owner     = {stecher},
  timestamp = {2007.08.12},
}

